        "CMD" ;
    ex:parcours_file "ALMA_M1",
        "CMD_M1" ;
    ex:parcours_level "M1" ;
    ex:parcours_string "M1 Architecture Logicielle (ALMA),M1 Smart Computing" ;
    ex:prerequisite "(s)" ;
    ex:responsible "Volume horaire total TOTAL : 47.98h Répartition : CM : 19.99h TD : 6.66h CI : 0h TP : 21.33h EAD : 0h" ;
    ex:semester "1" .

