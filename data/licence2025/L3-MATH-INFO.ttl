ex:UE_X32I030 a ex:UE ;
    rdfs:label "Recherche opérationnelle" ;
    ex:bibliography "" ;
    ex:code "X32I030" ;
    ex:content """fondamental :
-""" ;
    ex:evaluation "Recherche opérationnelle 100%" ;
    ex:hours "TOTAL : 46.2h Répartition : CM : 13.33h TD : 20.67h CI : 0h TP : 8h EAD : 4.2h" ;
